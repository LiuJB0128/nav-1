/* css reset */
*{box-sizing: border-box;}
*:before,*:after{box-sizing: border-box;}
*{margin:0; padding:0;}
ul,ol{ list-style: none; }

/* style */
body{background: #eee;}
.globalHeader { border: 1px solid red;
  margin: 30px;
  display: flex;
  justify-content: space-between;
}
.globalHeader > input {
    width: 100%;
    height: 46px;
    padding: 0 10px;
    position: absolute;
    width: 315px;
    height: 46px;
    left: 30px;
    top: 215px;

    background: #FFFFFF;
    border: 1px solid #DDDDDD;
    box-sizing: border-box;
    border-radius: 6px;
 }
.globalHeader > button {
    width: 32px;
    height: 32px;
    padding: 0px;
    border: 1px solid red;
    border: none;
    background: #FFFFFF;
    border-radius: 6px;
    position: absolute;
    left: 305px;
    top: 222px;
 }

.globalMain { }
.siteContainer .tile { }
.siteContainer .addButton { }
