/* css reset */
*{box-sizing: border-box;}
*:before,*:after{box-sizing: border-box;}
*{margin:0; padding:0;}
ul,ol{ list-style: none; }
/* style */
body{background: #eee;}
.globalHeader { border: 1px solid red;
  margin: 30px;
  display: flex;
  justify-content: space-between;
}
.globalHeader > input {
    width: 100%;
    height: 46px;
    padding: 0 10px;
    border: 1px solid #ddd;
    border-radius: 4px;
 }
.globalHeader > button {
    width:46px;
    padding: 0px;
    border: none;
    border-radius: 4px;
    position: absolute;
    width: 31.18px;
    height: 32px;
    left: 305px;
    top: 222px;

    background: url(05d568b9ce5d666b61f85f104973c9c0);
 }
